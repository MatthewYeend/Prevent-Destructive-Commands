{
    "name": "mattyeend/prevent-destructive-commands",
    "description": "A Laravel package to stop destructive commands.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Prevent",
        "Destructive",
        "Commands",
        "Prevent-Destructive-Commands",
        "Package"
    ],
    "authors": [
        {
            "name": "Matt Yeend",
            "email": "mail@mattyeend.co.uk"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^9.0 | ^10.0 | ^11.0",
        "illuminate/support": "^9.0 | ^10.0 | ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mattyeend\\PreventDestructiveCommands\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattYeend\\PreventDestructiveCommands\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MattYeend\\PreventDestructiveCommands\\PreventDestructiveCommandsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
